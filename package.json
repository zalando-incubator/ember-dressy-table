{
  "name": "ember-dynamic-table",
  "version": "0.0.1",
  "description": "Table component for dynamic tables.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "iaroslav.popov@zalando.de",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "sonar": "COVERAGE=true ember test;npm run replace;sonar-scanner -Dsonar.password=$(ztoken)",
    "replace": "node ./node_modules/replace-in-file/bin/cli.js '/SF:ember-dynamic-table/g' 'SF:addon' ./coverage/lcov.info --isRegex",
    "docs": "ember build -o docs/ -e docs"
  },
  "dependencies": {
    "ember-cli-babel": "^6.0.0-beta.3",
    "ember-truth-helpers": "^1.3.0",
    "ember-cli-htmlbars": "^1.3.3"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-funnel": "^2.0.1",
    "ember-ajax": "^2.4.1",
    "ember-array-helper": "^2.1.0",
    "ember-cli": "2.13.0-beta.3",
    "ember-cli-code-coverage": "^0.4.1",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0-beta.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-page-object": "^1.10.0",
    "ember-cli-qunit": "^4.0.0-beta.1",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-example-component": "^0.1.1",
    "ember-code-snippet": "^2.0.0-alpha.2",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.13.0-beta.1",
    "loader.js": "^4.2.3",
    "replace-in-file": "^2.5.3"
  },
  "engines": {
    "node": ">= 4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
